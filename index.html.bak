        async function saveToHistory(type, prompt, inputImages, resultImage) {
            console.log('saveToHistory is called:', { type, prompt, inputImages, resultImage });
            console.log('Current user:', currentUser);
            console.log('Current userHistory length:', userHistory ? userHistory.length: 'undefined');
            
            // Support unlogged users to save history
