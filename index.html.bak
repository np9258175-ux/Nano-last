            userHistory.unshift(historyItem); // Add to the beginning
            console.log('After adding to userHistory, the length becomes:', userHistory.length);
            
            // Save to localStorage to ensure data persistence
            saveUserHistoryToStorage();
            
            // Save to the backend at the same time
