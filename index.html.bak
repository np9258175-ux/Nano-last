        // Google login related functions
        function googleLogin() {
            // Check if you are already logged in
            if (currentUser) {
                console.log('User is logged in:', currentUser);
                return;
            }
            
            // Check whether the Google Client ID is configured
            if (GOOGLE_CLIENT_ID === 'YOUR_GOOGLE_CLIENT_ID') {
                showError('Google login is not configured. Please configure the Google OAuth client ID first.');
                console.error('Google OAuth Client ID not configured');
                return;
            }
            
            try {
                // Use Google Sign-In API to obtain user information
                // This method can directly obtain user's email, name, avatar and other information
                
                // Create Google Sign-In button
                const googleSignInButton = document.createElement('div');
                googleSignInButton.id = 'google-signin-button';
                googleSignInButton.style.cssText = `
                    position: fixed;
                    top: 0;
                    left: 0;
                    width: 100%;
                    height: 100%;
                    background: rgba(0, 0, 0, 0.5);
                    display: flex;
                    align-items: center;
                    justify-content: center;
                    z-index: 10000;
                `;
                
                const signInContent = document.createElement('div');
                signInContent.style.cssText = `
                    background: white;
                    padding: 30px;
                    border-radius: 15px;
                    box-shadow: 0 10px 30px rgba(0, 0, 0, 0.3);
                    text-align: center;
                `;
                
                signInContent.innerHTML = `
                    <h3 style="margin: 0 0 20px 0; color: #333;">Google account login</h3>
                    <p style="margin: 0 0 20px 0; color: #666;">Click the button below to log in with your Google account</p>
                    <div id="g_id_onload"
                         data-client_id="${GOOGLE_CLIENT_ID}"
                         data-context="signin"
                         data-ux_mode="popup"
                         data-callback="handleGoogleSignIn"
                         data-auto_prompt="false">
                    </div>
                    <div class="g_id_signin"
                         data-type="standard"
                         data-shape="rectangular"
                         data-theme="outline"
                         data-text="signin_with"
                         data-size="large"
                         data-logo_alignment="left">
                    </div>
                    <button onclick="closeGoogleSignInModal()" style="margin-top: 20px; padding: 10px 20px; border: none; border-radius: 8px; background: #6c757d; color: white; cursor: pointer;">Cancel</button>
                `;
                
                googleSignInButton.appendChild(signInContent);
                document.body.appendChild(googleSignInButton);
                
                // Loading the Google Sign-In API
                loadGoogleSignInAPI();
                
            } catch (error) {
                console.error('Google login error:', error);
                showError(`Google login error: ${error.message}`);
            }
        }
        
        // Loading the Google Sign-In API
        function loadGoogleSignInAPI() {
            if (window.google && window.google.accounts) {
                console.log('Google Sign-In API loaded');
                return;
            }
            
            const script = document.createElement('script');
            script.src = 'https://accounts.google.com/gsi/client';
            script.async = true;
            script.defer = true;
            script.onload = function() {
                console.log('Google Sign-In API Âä†ËΩΩÂÆåÊàê');
            };
            script.onerror = function() {
                console.error('Google Sign-In API loading failed');
                showError('Google login service failed to load, please try again');
            };
            document.head.appendChild(script);
        }
        
        // Close the Google login modal box
        function closeGoogleSignInModal() {
            const modal = document.getElementById('google-signin-button');
            if (modal) {
                document.body.removeChild(modal);
            }
        }
        
        // Handle Google Sign-In callbacks
        function handleGoogleSignIn(response) {
            console.log('Google Sign-In response:', response);
            
            if (response.credential) {
                // Decode the JWT token to obtain user information
                const payload = JSON.parse(atob(response.credential.split('.')[1]));
                console.log('Decoded user information:', payload);
                
                // Create user object - Use a stable mailbox hash as the ID to ensure that the same mailbox is the same ID every time you log in to the same mailbox
                const emailHash = btoa(payload.email).replace(/[^a-zA-Z0-9]/g, '');
                const stableUserId = `google_${emailHash}`;
                
                const googleUser = {
                    id: stableUserId,
                    email: payload.email,
                    name: payload.name || payload.email.split('@')[0],
                    avatar: payload.picture,
                    googleId: payload.sub,
                    picture: payload.picture
                };
                
                console.log('Created user object:', googleUser);
                
                // Close the modal box
                closeGoogleSignInModal();
                
                // Log in to the user
                loginUser(googleUser);
                
            } else {
                console.error('Google Sign-In failed: No credentials were obtained');
                showError('Google login failed, please try again');
            }
        }
        
        // Check whether there is existing user data with the same mailbox
        function checkExistingUserData(email) {
            console.log('Check existing user data of the mailbox:', email);
            
            // Check whether there are users with the same mailbox in localStorage
            const savedUser = localStorage.getItem('nanoBananaUser');
            if (savedUser) {
                try {
                    const user = JSON.parse(savedUser);
                    if (user.email === email) {
                        console.log('Finding the user with the same mailbox in localStorage:', user);
                        return user;
                    }
                } catch (error) {
                    console.error('Failed to parse saved user information:', error);
                }
            }
            
            // Check whether there is a history of the same mailbox
            for (let i = 0; i < localStorage.length; i++) {
                const key = localStorage.key(i);
                if (key && key.includes('nanoBananaHistory')) {
                    try {
                        const historyData = localStorage.getItem(key);
                        if (historyData) {
                            const history = JSON.parse(historyData);
                            // If there is data in the history, it means that the user has logged in before
                            if (history.length > 0) {
                                // Try to extract email information from the key
                                const keyParts = key.split('_');
                                if (keyParts.length >= 2) {
                                    const potentialEmail = atob(keyParts[1]);
                                    if (potentialEmail === email) {
                                        console.log('Finding the history of the same mailbox, user ID:', keyParts[1]);
                                        return { id: keyParts[1] };
                                    }
                                }
                            }
                        }
                    } catch (error) {
                        console.error('Check history failed:', error);
                    }
                }
            }
            
            console.log('No existing user data for the same mailbox was found');
            return null;
        }
        

        

        

        
        async function loginUser(user) {
            console.log('=== User login ===');
            console.log('Login user information:', user);
            
            // üöÄ Save the current era stamp for subsequent inspection
            const myEpoch = ++authEpoch;
            console.log('Login process starts, era stamp:', myEpoch);
            
            // üöÄ Check whether it is logging out. If so, stop the login process
            if (isLoggingOut) {
                console.log('Log out is detected, stop login process');
                return;
            }
            
            // Check whether there is user data of the same email address
            const existingUserData = checkExistingUserData(user.email);
            if (existingUserData) {
                console.log('Finding existing user data for the same mailbox:', existingUserData);
                // Merge user data and keep history
                user = { ...existingUserData, ...user };
                console.log('Merged user data:', user);
            }
            
            // If other users have logged in, clean it up first
            if (currentUser && currentUser.id !== user.id) {
                console.log('Detected different user login, clean up previous data');
                // Save previous user history
                if (userHistory.length > 0) {
                    saveUserHistoryToStorage();
                    console.log('The history of previous users has been saved');
                }
                // Clear the current history array
                userHistory = [];
            }
            
            // Set up new users
            currentUser = user;
            console.log('The current user has been set to:', currentUser.id);
            
            // Update UI display
            updateLoginUI();
            
            // Show history main tab page
            document.getElementById('history-main-tab').style.display = 'inline-block';
            
            // üöÄ Fix race condition: Wait for all data to load before saving the login status
            // This ensures user data integrity and avoids race conditions
            
            // Synchronize user information to the backend
            if (config) {
                console.log('Sync user information to the backend...');
                await upsertUserToSupabase(user);
                
                // üöÄ Fuse 1: Check if it is logged out and era poke after upsert
                if (isLoggingOut || !currentUser || myEpoch !== authEpoch) {
                    console.log('Log out, user status abnormality or era stamp mismatch is detected, stop the login process');
                    return;
                }
            }
            
            // Load user history
            await loadUserHistory();
            
            // üöÄ Fuse 2: After loadUserHistory, check whether it is logged out and era poke
            if (isLoggingOut || !currentUser || myEpoch !== authEpoch) {
                console.log('Log out, user status abnormality or era stamp mismatch is detected, stop the login process');
                return;
            }
// üöÄ Save the login status to localStorage now safely
            // All data has been loaded and the user status is complete
            localStorage.setItem('nanoBananaUser', JSON.stringify(user));
            console.log('All data is loaded and the login status has been saved to localStorage');
            
            // üöÄ Fixed: Reset the logout flag after successful login to ensure consistency
            isLoggingOut = false;
            console.log('Login completed successfully, reset the logout flag');
            
            showSuccessMessage(`Welcome back, ${user.name}!`);
        }
        
                function googleLogout() {
            console.log('=== User logout ===');
            
            // üöÄ Set the logout flag to prevent race conditions
            isLoggingOut = true;
            console.log('Set logout flag to prevent the login process from continuing execution');
            
            // üöÄ Add an era stamp to automatically invalidate all old login processes
            authEpoch++;
            console.log('Era stamp has increased, current era:', authEpoch);
            
            // üöÄ Bottom: Force close the load prompt to avoid residual
            const loading = document.getElementById('history-loading');
            const list = document.getElementById('history-list');
            if (loading) {
                loading.style.display = 'none';
                console.log('Forced to close the history loading prompt');
            }
            if (list) {
                list.style.display = 'block';
                console.log('Restore history list display');
            }
            
            // Save the current user's history before logging out
            if (currentUser && userHistory.length > 0) {
                console.log('Save history before logging out, user ID:', currentUser.id);
                saveUserHistoryToStorage();
                console.log('Save history');
            }
            
            // Clean up all user-related data
            const oldUserId = currentUser ? currentUser.id : null;
            currentUser = null;
            userHistory = [];
            
            console.log('User data has been cleaned, old user ID:', oldUserId);
            
            // Update UI display
            updateLoginUI();
            
            // Hide history main tab page
            document.getElementById('history-main-tab').style.display = 'none';
            
