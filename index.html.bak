    <script>
        console.log('Script starting...');
        
        // API configuration - now proxy through local server
        const API_BASE = window.location.origin || ''; // Use the current domain name to avoid undefined
        const MODEL = 'gemini-2.5-flash-image-preview';
        
        let currentImageData = null;
