        // History related functions
        let _loadingHistory = false; // üöÄ Lightweight reentry lock to prevent concurrent loading
        
        async function loadUserHistory() {
            // Support unlogged user loading history
            console.log('Loading history (support anonymous users)');
            
            // üöÄ Prevent concurrent loading from causing "chrysanthemum flickering"
            if (_loadingHistory) {
                console.log('History is loading, skip repeated calls');
                return;
            }
            _loadingHistory = true;
            
            // üöÄ Show history loading prompt
            showHistoryLoading();
            
            try {
                // Set synchronization status
                window.isHistorySyncing = true;
                
                // Try to quickly load from localStorage first (user experience priority)
                const hasLocalHistory = loadUserHistoryFromStorage();
                if (hasLocalHistory && userHistory.length > 0) {
                    renderHistory();
                    console.log('Quick loading: load history from localStorage, total', userHistory.length, 'bar');
                }
                
                // The background synchronizes the latest data from the backend (not blocking the UI)
                if (config) {
                    await loadHistoryFromSupabaseBackground();
                }
                
                // Synchronization is completed, clear the status
                window.isHistorySyncing = false;
                
                // Re-render to display the final result
                renderHistory();
            } catch (error) {
                console.error('Loading history failed:', error);
                // Synchronization is completed, clear the status
                window.isHistorySyncing = false;
                
                // Re-render to display the final result
                renderHistory();
            } finally {
                // üöÄ Whether successful or failure, hide the history loading prompt
                hideHistoryLoading();
                _loadingHistory = false; // üöÄ Release the reentry lock
            }
        }
        
        // üöÄ Show history loading prompt
        let _loadingShowTime = 0; // Record the load start time
        function showHistoryLoading() {
            // Check whether the logout is being logged out or the era stamp does not match. If so, the loading prompt will not be displayed.
            if (isLoggingOut || !currentUser) {
                console.log('Login or user status abnormality was detected, no loading prompt was displayed');
                return;
            }
            
            const historyLoading = document.getElementById('history-loading');
            const historyList = document.getElementById('history-list');
            
            if (historyLoading && historyList) {
                // üöÄ Record the display start time, used to prevent flickering
                _loadingShowTime = Date.now();
                
                historyLoading.style.display = 'block';
                historyList.style.display = 'none';
                console.log('Show history loading prompt');
                
                // üöÄ Make sure the loading prompt is displayed when currently on the History tab
                const historyTab = document.getElementById('history-tab');
                if (historyTab && !historyTab.classList.contains('active')) {
                    console.log('It is not currently in the History tab, hide the loading prompt');
                    historyLoading.style.display = 'none';
                    historyList.style.display = 'block';
                    _loadingShowTime = 0; // Reset time
                }
            }
        }
        
        // üöÄ Hide history loading prompts
        function hideHistoryLoading() {
            // Check whether the logout is being logged out or the era stamp does not match. If so, the loading prompt will not be hidden
            if (isLoggingOut || !currentUser) {
                console.log('Login out or user status abnormality was detected, and the loading prompt is not hidden');
                return;
            }
            
            const historyLoading = document.getElementById('history-loading');
            const historyList = document.getElementById('history-list');
            
            if (historyLoading && historyList) {
                // üöÄ Anti-flashing: Make sure that the loading prompt is displayed at least 300ms
                const showDuration = Date.now() - _loadingShowTime;
                const minShowTime = 300; // Minimum display of 300ms
                
                if (_loadingShowTime > 0 && showDuration < minShowTime) {
                    const remainingTime = minShowTime - showDuration;
                    console.log(`Anti-flash delay: ${showDuration}ms is displayed, hidden after delay ${remainingTime}ms);
                    
                    setTimeout(() => {
                        // Check the status again to make sure that the delayed period is not logged out
                        if (!isLoggingOut && currentUser && historyLoading && historyList) {
                            historyLoading.style.display = 'none';
                            historyList.style.display = 'block';
                            console.log('Hide history loading prompt after delay');
                        }
                        _loadingShowTime = 0; // Reset time
                    }, remainingTime);
                } else {
                    historyLoading.style.display = 'none';
                    historyList.style.display = 'block';
                    console.log('Hide history loading prompt');
                    _loadingShowTime = 0; // Reset time
                }
            }
        }
        
        // Backend loading Supabase data without blocking the UI - Fixed version
        async function loadHistoryFromSupabaseBackground() {
            // Show background synchronization status
            showHistorySyncStatus();
            
            try {
                let response;
                
                if (currentUser) {
                    // Logged in users use the original API
                    console.log('Start loading history from backend API, user ID:', currentUser.id);
                    response = await fetch('/api/user-history-unified?simple=true', {
                        method: 'GET',
                        headers: {
                            'user-id': currentUser.id
                        }
                    });
                } else {
                    //Unlogged in user uses new anonymous API
                    console.log('Start loading anonymous user history from the backend API');
                    response = await fetch('/api/get-anonymous-history?simple=true', {
                        method: 'GET'
                    });
                }
                
                const result = await response.json();
                
                if (!response.ok) {
                    console.error('Failed to load history from backend:', result.error);
                    hideHistorySyncStatus();
                    return;
                }
                
                const data = result.data;
                
                if (data && data.length > 0) {
                    console.log('Backend API returns the original data:', data);
                    
                    // Convert data format
                    const newHistory = data.map(item => ({
                        id: String(item.id),
                        type: item.type,
                        prompt: item.prompt,
                        resultImage: item.result_image,
                        inputImages: item.input_images || [],
                        createdAt: item.created_at
                    }));
                    
                    // Update local data (data pulled from the cloud is not saved to localStorage to avoid storage space issues)
                    userHistory = newHistory;
                    console.log('Background synchronization: cloud data has been updated to memory, total', userHistory.length, 'bar');
                    console.log('Note: cloud data will not be saved to localStorage, only newly generated records will be saved');
                    
                    //Re-render (if the user is currently on the History page)
                    const historyTab = document.querySelector('.main-tab-button[onclick*="history"]');
                    if (historyTab && historyTab.classList.contains('active')) {
                        renderHistory();
                        console.log('Backend Synchronization: Backend API data has been updated, total ', userHistory.length, 'bar');
                    }
                } else {
                    console.log('No history found in the backend API');
                }
                
                hideHistorySyncStatus();
            } catch (error) {
                console.error('backend synchronization failed:', error);
                hideHistorySyncStatus();
            }
        }
