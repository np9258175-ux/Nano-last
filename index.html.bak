        // Clean up old data in localStorage
        function cleanupLocalStorage() {
            try {
                const keysToRemove = [];
                for (let i = 0; i < localStorage.length; i++) {
                    const key = localStorage.key(i);
                    if (key && key.includes('nanoBananaHistory')) {
                        // Check the data size
                        const data = localStorage.getItem(key);
                        if (data && data.length > 1000000) { // If the data is greater than 1MB
                            keysToRemove.push(key);
                            console.log('cleanupLocalStorage: Tag to delete large files:', key, 'size:', data.length);
                        }
                    }
                }
                
