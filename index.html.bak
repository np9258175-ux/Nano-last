        };
        
        // Google login related variables
        let currentUser = null;
        let userHistory = [];
        let isLoggingOut = false; // Flag to prevent login/logout of race conditions
        let authEpoch = 0; // üöÄ Time stamp/round anti-shake to prevent interference from old login process
        
        // Google OAuth 2.0 configuration
        // Please follow the steps below to configure:
        // 1. Visit https://console.cloud.google.com/
        // 2. Create a project or select an existing project
        // 3. Enable the Google+ API and Google OAuth2 API
        // 4. Create OAuth 2.0 client ID on the "Credentials" page
        // 5. Set up authorized domain names (including your development domain names)
        // 6. Replace the obtained client ID with the following value
        //
        // üöÄ Vercel deployment configuration:
        // Authorized JavaScript source: https://nano-banana-tool.vercel.app
        // Authorization redirection URI: https://nano-banana-tool.vercel.app/oauth-callback.html
        // Google OAuth configuration - get from backend API
        let GOOGLE_CLIENT_ID = null;
        const GOOGLE_SCOPE = 'openid email profile';
        
        // Initialize the Supabase client - securely obtain configuration from the backend API
        let supabase = null;
        let config = null;
        
        // Get configuration from the backend
        async function initializeSupabase() {
            try {
                const response = await fetch('/api/config');
                if (!response.ok) {
                    throw new Error(`HTTP error! status: ${response.status}`);
                }
                config = await response.json();
                
                if (config.supabaseUrl && config.supabaseAnonKey) {
                    supabase = window.supabase.createClient(config.supabaseUrl, config.supabaseAnonKey);
                    console.log('‚úÖ Supabase client is securely initialized');
                } else {
                    throw new Error('Configuration information is incomplete');
                }
                
                // Set Google Client ID
                if (config.googleClientId) {
                    GOOGLE_CLIENT_ID = config.googleClientId;
                    console.log('‚úÖ Google Client ID is loaded safely');
                }
            } catch (error) {
                console.error('‚ùå Supabase client initialization failed:', error);
                console.warn('‚ö†Ô∏è localStorage will be used as an alternative');
            }
        }

        // Switch the main tab (Editor/History)
        function switchMainTab(tabId, event) {
            console.log('switchMainTab called with:', tabId, event);
            
            try {
                // Hide all main tag content
                document.querySelectorAll('.main-tab-content').forEach(content => {
                    content.classList.remove('active');
                });
                
                // Remove all active classes of main tag buttons
                document.querySelectorAll('.main-tab-button').forEach(button => {
                    button.classList.remove('active');
                });
                
                // Show the selected main tag content
                const targetTab = document.getElementById(tabId + '-tab');
                if (targetTab) {
                    targetTab.classList.add('active');
                } else {
                    console.error('Target main tab not found:', tabId + '-tab');
                    return;
                }
                
                // Activate the corresponding main tag button
                if (event && event.target) {
                    event.target.classList.add('active');
                }
                
                // Clear previous results and errors
                hideResult();
                hideError();
                
                // Hide success message
                const successMessage = document.getElementById('success-message');
                if (successMessage) {
                    successMessage.classList.remove('show');
                }
                
                // üöÄ When switching to Editor, check whether it is still being generated and automatically restore the generation status
                if (tabId === 'editor') {
                    if (globalGenerationState.isGenerating) {
                        console.log('Detected that the Editor is still being generated, and the generated state is automatically restored:', globalGenerationState.message);
                        showGenerationStatus(globalGenerationState.message);
                    }
                }
                
                // üöÄ When switching to History, check whether it is still in synchronization/request, and "Loading" is automatically displayed
                if (tabId === 'history') {
                    if (_loadingHistory || window.isHistorySyncing) {
                        console.log('History is detected that it is still in synchronization, and the load status is automatically displayed');
                        showHistoryLoading();
                    }
                }
                
                console.log('Main tab switched successfully to:', tabId);
