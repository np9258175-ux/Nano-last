                if (historyItem) {
                    console.log('History found in userHistory array');
                }
            } else {
                console.log('userHistory array is empty or undefined, try to reload...');
                // Try to reload the history
                loadUserHistoryFromStorage();
                if (userHistory && userHistory.length > 0) {
                    console.log('The length of userHistory after reloading:', userHistory.length);
