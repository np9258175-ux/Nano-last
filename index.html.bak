                    console.error('4. The data has been cleared from localStorage');
                    showError('Please refresh the interface first and then click on the reference');
                    return;
                }
            }
            
            console.log('reference history:', historyItem);
            
            // Switch to the Editor tab image editing function
            switchMainTab('editor');
            switchEditorTab('multi-image');
            
            // Clear the current selection
            selectedMultiFiles = [];
            
            // Only reference the result image, not the original input image
            if (historyItem.resultImage) {
                try {
                    // Convert base64 to File object
                    const base64Data = historyItem.resultImage.split(',')[1];
                    const byteCharacters = atob(base64Data);
                    const byteNumbers = new Array(byteCharacters.length);
                    for (let i = 0; i < byteNumbers.length; i++) {
                        byteNumbers[i] = byteCharacters.charCodeAt(i);
                    }
                    const byteArray = new Uint8Array(byteNumbers);
                    const blob = new Blob([byteArray], { type: 'image/png' });
                    const file = new File([blob], `referenced_result_${Date.now()}.png`, { type: 'image/png' });
                    
                    selectedMultiFiles.push(file);
                    console.log('Result image has been added as input:', file.name);
                } catch (error) {
                    console.error('Convert result image failed:', error);
                    showError('Cannot reference image, please try again');
                    return;
                }
            }
            
            // No more original input image is added, only the result image is referenced
            console.log('Reference strategy: only reference the result image, not the original input image');
            
            // Do not fill in the prompt words, let the user enter it by himself
            console.log('Quotation strategy: only reference images, not fill in prompt words, users need to enter by themselves');
            
            // Update preview
            updateMultiImagePreview();
        // Show success message
            const imageCount = selectedMultiFiles.length;
            showSuccessMessage(`Referenced history, a total of ${imageCount} images, you can continue editing or generating new images`);
            
            console.log('Reference is completed, currently selected image number:', selectedMultiFiles.length);
        }
        
        // Find history in localStorage
        function findHistoryInLocalStorage(historyId) {
            console.log('Try to find history in localStorage:', historyId);
            
            if (!currentUser) {
                console.log('No current user, localStorage cannot be found');
                return null;
            }
            
            console.log('Current user ID:', currentUser.id);
            console.log('localStorage Total Number of Keys:', localStorage.length);
            
            // Priority search for the current user's history key
            const currentUserKey = `nanoBananaHistory_${currentUser.id}`;
            console.log('The current user's history key:', currentUserKey);
            
            // First try to find the keys of the current user
            try {
                const currentUserHistory = localStorage.getItem(currentUserKey);
                if (currentUserHistory) {
                    const history = JSON.parse(currentUserHistory);
                    console.log(`The current user key contains ${history.length} history`);
                    
                    if (history.length > 0) {
                        console.log('ID of the first 3 records:');
                        history.slice(0, 3).forEach((item, index) => {
                            console.log(` [${index}] ID: "${item.id}", type: ${typeof item.id}`);
                        });
                        
                        const foundItem = history.find(item => {
                            const match = item.id === historyId;
                            console.log(`Compare the current user key: "${item.id}" === "${historyId}" => ${match}`);
                            return match;
                        });
                        
                        if (foundItem) {
                            console.log('History found in the current user key:', foundItem);
                            return foundItem;
                        }
                    }
                } else {
                    console.log('There is no history data in the current user key');
                }
            } catch (error) {
                console.error('Failed to parse the current user history:', error);
            }
            
            // If the current user key is not found, traverse all history keys (for debugging)
            console.log('Not found in the current user key, check all history keys...');
            const historyKeys = [];
            for (let i = 0; i < localStorage.length; i++) {
                const key = localStorage.key(i);
                if (key && key.includes('nanoBananaHistory')) {
                    historyKeys.push(key);
                    console.log('History key found:', key);
                }
            }
            
            console.log('all history keys:', historyKeys);
            
            // Iterate through each history key (for debugging only, no data from other users)
            for (const key of historyKeys) {
                if (key === currentUserKey) continue; // Skip the current user key
                
                try {
                    const historyData = localStorage.getItem(key);
                    if (historyData) {
                        const history = JSON.parse(historyData);
                        console.log(`key ${key} contains ${history.length} history (other users)`);
                        
                        // Display the IDs of the first few records for debugging
                        if (history.length > 0) {
                            console.log('ID of the first 3 records:');
                            history.slice(0, 3).forEach((item, index) => {
                                console.log(` [${index}] ID: "${item.id}", type: ${typeof item.id}`);
                            });
                        }
                        
                        // Find matching IDs in history (for debugging only)
                        const foundItem = history.find(item => {
                            const match = item.id === historyId;
                            console.log(`Compare key ${key}: "${item.id}" === "${historyId}" => ${match}`);
                            return match;
                        });
                        
                        if (foundItem) {
                            console.log('Finding matching history in other user keys (data isolation problem):', foundItem);
                            console.log('‚ö†Ô∏è Warning: This may indicate a problem with data isolation');
                            // Do not return data from other users, keep data isolated
                        }
                    }
                } catch (error) {
                    console.error('parse localStorage key', key, 'failed:', error);
                }
            }
            
            console.log('No matching history found in localStorage');
            return null;
        }
        
