                    historyItem = userHistory.find(item => item.id === historyId);
                    if (historyItem) {
                        console.log('History found in userHistory after reloading');
                    }
                }
            }
            
            // If not found in userHistory, try to find it in localStorage
            if (!historyItem) {
                console.log('Not found in userHistory, try to find in localStorage...');
                historyItem = findHistoryInLocalStorage(historyId);
                
