                            alert('Format is not supported, please use JPEG or PNG format');
                            return;
                        }
                        
                        if (file.size > 10 * 1024 * 1024) {
                            alert('The file is too large, please select an image less than 10MB');
                            return;
                        }
                        
                        // Set the file
                        selectedSingleFile = file;
                        updateSingleImagePreview();
                        console.log('Single image drag and drop: file setting is successful');
                    }
                });
            } else {
                console.error('Single image function: single-image-display or single-image element not found');
            }
        });

        // Helper functions for handling drag and drop files
        function processDroppedFile(file) {
            // Verify file format
            if (file.type !== 'image/jpeg' && file.type !== 'image/jpg' && file.type !== 'image/png') {
                alert(`file ${file.name} format is not supported. Please use JPEG or PNG format.`);
                return false;
            }
            
            // Check file size
            if (file.size > 10 * 1024 * 1024) {
                alert(`file ${file.name} is too large. Please select an image less than 10MB.`);
                return false;
            }
            
            // Check whether the file with the same name already exists
            const existingFile = selectedMultiFiles.find(f => f.name === file.name);
            if (existingFile) {
                alert(`file ${file.name} already exists, skip`);
                return false;
            }
            
            // Add files to array
            selectedMultiFiles.push(file);
            return true;
        }
    </script>
</body>
</html>