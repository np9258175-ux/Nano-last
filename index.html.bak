                    console.log('No history found in Supabase');
                    return false;
                }
            } catch (error) {
                console.error('Supabase history loading error:', error);
                return false;
            }
        }
        

        
        // Delete history items
        async function deleteHistoryItem(historyId) {
            if (!currentUser) {
                showError('Please log in first');
                return;
            }
            
            if (!confirm('Are you sure you want to delete this history? This operation is not restored.')) {
                return;
            }
            
            try {
                console.log('=== Delete history debug information ===');
                console.log('historyId to be deleted:', historyId);
                console.log('Current userHistory array:', userHistory);
                console.log('userHistory length:', userHistory ? userHistory.length: 'undefined');
                
                if (userHistory && userHistory.length > 0) {
                    console.log('ID of all items in userHistory:');
                    userHistory.forEach((item, index) => {
                        console.log(` [${index}] ID: "${item.id}", type: ${typeof item.id}`);
                    });
                }
                
                // Remove from the backend API
                if (config) {
                    const numericId = parseInt(historyId);
                    if (isNaN(numericId)) {
                        console.error('Invalid ID format:', historyId);
                        showError('Delete failed: Invalid record ID format');
                        return;
                    }
                    
                    const response = await fetch('/api/user-history-unified', {
                        method: 'DELETE',
                        headers: {
                            'user-id': currentUser.id,
                            'Content-Type': 'application/json'
                        },
                        body: JSON.stringify({
                            id: numericId
                        })
                    });
                    
                    const result = await response.json();
                    if (!response.ok) {
                        console.error('Deletion from backend failed:', result.error);
                        showError('Deletion failed:' + result.error);
                        return;
                    }
                    
                    console.log('History has been deleted from the backend');
                }
                
                // Delete from local array
                const index = userHistory.findIndex(item => {
                    const match = item.id === historyId;
                    console.log(`Compare: "${item.id}" === "${historyId}" => ${match}`);
                    return match;
                });
                
                console.log('Index Found:', index);
                
                if (index !== -1) {
                    // Delete from local array
                    userHistory.splice(index, 1);
                    console.log('History has been deleted from local array, new length:', userHistory.length);
                    
                    // Update localStorage
                    saveUserHistoryToStorage();
                    
                    // Force re-render history
                    console.log('Prepare to re-render the history...');
                    renderHistory();
                    console.log('History interface has been rerendered');
                    
                    // Confirm the interface update again
                    setTimeout(() => {
                        console.log('Delay check interface status...');
                        const currentHistoryItems = document.querySelectorAll('.history-item');
                        console.log('The number of history items displayed on the current interface:', currentHistoryItems.length);
                        console.log('userHistory array length:', userHistory.length);
                        
                        if (currentHistoryItems.length !== userHistory.length) {
                            console.log('The interface is out of sync with the data, force re-rendering');
                            renderHistory();
                        }
                    }, 200);
                    
                    showSuccessMessage('History deleted');
                } else {
                    console.error('No matching history item found');
                    console.error('Possible Cause:');
                    console.error('1. historyId type mismatch');
                    console.error('2. The userHistory array is empty or undefined');
                    console.error('3. The ID format of the history item is inconsistent');
                    
                    // Even if it is not found, try refreshing the interface to prevent the data from being synchronized
                    console.log('Try to force refresh the interface...');
                    setTimeout(() => {
                        renderHistory();
                        console.log('The interface has been forced to refresh');
                    }, 100);
                    
                    showError('Deleted, please refresh the interface');
                }
                
                console.log('=== Delete history debugging information ends ===');
                
            } catch (error) {
                console.error('Delete history error:', error);
                showError('Deletion failed:' + error.message);
            }
        }
        
        // Clear all history
        async function clearAllHistory() {
            if (!currentUser) {
                showError('Please log in first');
                return;
            }
            
            if (!confirm('Are you sure you want to clear all history? This operation is not recoverable.')) {
                return;
            }
            
